{
  "tagname-lowercase": true,
  "attr-lowercase": true,
  "attr-value-double-quotes": true,
  "doctype-first": false,
  "tag-pair": true,
  "spec-char-escape": true,
  "id-unique": true,
  "src-not-empty": true,
  "attr-unsafe-chars": true,
  "space-tab-mixed-disabled": true,
  "attr-value-not-empty": false,
  "tag-self-close": false,
  "head-script-disabled": true,
  "title-require": false,
  "alt-require": true,
  "doctype-html5": true,
  "id-class-ad-disabled": true,
  "jshint": {
    "boss": true,
    "browser": true,
    "camelcase": false,
    "curly": false,
    "eqeqeq": true,
    "eqnull": true,
    "esversion": 6,
    "immed": true,
    "latedef": false,
    "newcap": false,
    "noarg": true,
    "nonbsp": true,
    "nonew": false,
    "plusplus": false,
    "undef": true,
    "unused": true,
    "strict": false,
    "trailing": false,
    "smarttabs": true,
    "laxbreak": true
  }
}